A={1:[1650,'likki','software',30000,'kakinada'],2:[1655,'likitha','doctor',80000,'vizag'],3:[1765,'priya','police',70000,'agra'],4:[1785,'pitta','teacher',90000,'kakinada']}
x=int(input('enter a number:'))
print(x)
print('employee number:',A[x][0])
print('name of the person:',A[x][1])
print('employee job:',A[x][2])
print('employee salary:',A[x][3])
print('city:',A[x][4])
